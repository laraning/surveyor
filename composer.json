{
    "name": "laraning/surveyor",
    "description": "Surveyor - An user profile roles and permissions administration package.",
    "type": "library",
    "keywords": [
        "utilities",
        "waygou",
        "laravel",
        "php"
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^5.6",
        "laraning/boost": "^1.0",
        "predis/predis": "^1.1",
        "spatie/laravel-permission": "^2.17",
        "tightenco/parental": "0.4-alpha.2",
        "predis/predis": "^1.1",
        "vyuldashev/nova-permission": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Laraning\\Surveyor\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laraning\\Cheetah\\SurveyorServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Laraning",
            "email": "bruno.falcao@laraning.com"
        }
    ],
    "minimum-stability": "dev"
}
