{
    "name": "laraning/surveyor",
    "description": "Surveyor - Manage your global scopes and policies in a Laravel application.",
    "type": "library",
    "keywords": [
        "laraning",
        "laravel",
        "php"
    ],
    "require": {
        "php": "^7.1",
        "laraning/boost": "*",
        "illuminate/support": "5.7.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Laraning\\Surveyor\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laraning\\Surveyor\\SurveyorServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Laraning",
            "email": "bruno.falcao@laraning.com"
        }
    ],
    "minimum-stability": "dev"
}
